generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           Int    @id @default(autoincrement())
  username     String @unique
  passwordHash String
}

model Session {
  id        String   @id
  sid       String   @unique
  data      String
  expiresAt DateTime
}
